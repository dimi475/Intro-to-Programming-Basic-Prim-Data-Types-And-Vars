using System;


class exchangeIntValues
{
    static void Main()
    {
        int a = 5;
        int b = 10;
        int c;
        Console.WriteLine("start values are a = {0} , b = {1}", a, b);
        c = a;
        a = b;
        b = c;
        Console.WriteLine("end values are a = {0} , b = {1}", a, b);

    }
}
